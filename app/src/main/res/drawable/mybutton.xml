<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#871313" />
            <gradient android:angle="-90" android:startColor="#593434" android:endColor="#9A6868"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#795E5E" />
            <solid android:color="#855858"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#795E5E" />
            <gradient android:angle="-90" android:startColor="#BA8D8D" android:endColor="#855858" />
        </shape>
    </item>
</selector>